productionVertex :
{
	type = "diffractiveDissVertex";
  beam : {
		name = "K-";
	};
  target : {
		name = "p+";
	};
};


decayVertex :
{

  XQuantumNumbers :
	{
		isospin     =  1;
		G           =  0;
		J           =  4;
		P           = -1;
		C           =  0;
		M           =  0;
		refl        = +1;
		strangeness = -1;
	};
  
  XDecay :
	{
		isobars = ({
				name = "f2(1270)";
				fsParticles = ({
						name = "pi-";
					},
					{
						name = "pi+";
					});
				L = 4;
				S = 0;
			});
		L = 0;
		S = 4;
		fsParticles = ({
				name = "K-";
			});
	};

};
